{
  "$schema": "https://developer.microsoft.com/json-schemas/spfx-build/config.2.0.schema.json",
  "version": "2.0",
  "bundles": {
    "document-library-web-part": {
      "components": [
        {
          "entrypoint": "./lib/webparts/documentLibrary/DocumentLibraryWebPart.js",
          "manifest": "./src/webparts/documentLibrary/DocumentLibraryWebPart.manifest.json"
        }
      ]
    },
    "pagination-web-part": {
      "components": [
        {
          "entrypoint": "./lib/webparts/pagination/PaginationWebPart.js",
          "manifest": "./src/webparts/pagination/PaginationWebPart.manifest.json"
        }
      ]
    }
  },
  "externals": {},
  "localizedResources": {
    "DocumentLibraryWebPartStrings": "lib/webparts/documentLibrary/loc/{locale}.js",
    "PaginationWebPartStrings": "lib/webparts/pagination/loc/{locale}.js"
  }
}
